{
  "name": "starter-microservice",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "engines": {
    "node": "18"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:devnet": "npm run copy-devnet-config & nest start",
    "start:devnet:watch": "npm run copy-devnet-config & nest start --watch",
    "start:devnet:debug": "npm run copy-devnet-config & nest start --watch --debug",
    "start:testnet": "npm run copy-testnet-config & nest start",
    "start:testnet:watch": "npm run copy-testnet-config & nest start --watch",
    "start:testnet:debug": "npm run copy-testnet-config & nest start --watch --debug",
    "start:mainnet": "npm run copy-mainnet-config & nest start",
    "start:custom": "npm run copy-custom-config & nest start",
    "start:custom:watch": "npm run copy-custom-config & nest start --watch",
    "start:custom:debug": "npm run copy-custom-config & nest start --watch --debug",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "copy-devnet-config": "cp ./config/config.devnet.yaml ./config/config.yaml",
    "copy-testnet-config": "cp ./config/config.testnet.yaml ./config/config.yaml",
    "copy-mainnet-config": "cp ./config/config.mainnet.yaml ./config/config.yaml",
    "copy-custom-config": "cp ./config/config.custom.yaml ./config/config.yaml"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.501.0",
    "@itheum/sdk-mx-data-nft": "2.6.0",
    "@lighthouse-web3/sdk": "^0.3.0",
    "@metaplex-foundation/mpl-token-metadata": "^3.1.2",
    "@metaplex-foundation/umi": "^0.9.0",
    "@metaplex-foundation/umi-bundle-defaults": "^0.9.0",
    "@multiversx/sdk-core": "12.18.0",
    "@multiversx/sdk-nestjs-auth": "3.1.3",
    "@multiversx/sdk-nestjs-cache": "3.1.3",
    "@multiversx/sdk-nestjs-common": "3.1.3",
    "@multiversx/sdk-nestjs-elastic": "3.1.3",
    "@multiversx/sdk-nestjs-http": "3.1.3",
    "@multiversx/sdk-nestjs-monitoring": "3.1.3",
    "@multiversx/sdk-nestjs-rabbitmq": "3.1.3",
    "@multiversx/sdk-nestjs-redis": "3.1.3",
    "@multiversx/sdk-network-providers": "2.2.1",
    "@multiversx/sdk-transaction-decoder": "1.0.2",
    "@multiversx/sdk-transaction-processor": "0.1.33",
    "@nestjs/bull": "10.0.1",
    "@nestjs/common": "10.3.1",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.3.1",
    "@nestjs/microservices": "10.3.1",
    "@nestjs/mongoose": "10.0.2",
    "@nestjs/platform-express": "^10.3.1",
    "@nestjs/platform-socket.io": "10.3.1",
    "@nestjs/schedule": "4.0.0",
    "@nestjs/swagger": "7.2.0",
    "@nestjs/typeorm": "10.0.1",
    "@nestjs/websockets": "10.3.1",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.89.1",
    "agentkeepalive": "4.5.0",
    "bignumber.js": "9.1.2",
    "bull": "4.12.2",
    "cache-manager": "5.4.0",
    "cookie-parser": "1.4.6",
    "cron": "3.1.6",
    "crypto": "1.0.1",
    "ethers": "6.10.0",
    "ioredis": "5.3.2",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "mysql2": "3.8.0",
    "nest-winston": "1.9.4",
    "prom-client": "15.1.0",
    "reflect-metadata": "0.2.1",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "socket.io": "4.7.4",
    "swagger-ui-express": "5.0.0",
    "typeorm": "0.3.19",
    "winston": "3.11.0",
    "winston-daily-rotate-file": "4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.0",
    "@nestjs/schematics": "10.1.0",
    "@nestjs/testing": "10.3.1",
    "@types/cache-manager": "4.0.6",
    "@types/cookie-parser": "1.4.6",
    "@types/cron": "2.0.1",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/js-yaml": "4.0.9",
    "@types/jsonwebtoken": "9.0.5",
    "@types/lodash": "4.14.202",
    "@types/multer": "^1.4.11",
    "@types/node": "20.11.6",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "js-yaml": "4.1.0",
    "prettier": "3.2.4",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3",
    "webpack": "^5.90.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
